  <UsingTask TaskName="lua51_Contains" AssemblyFile="$(MSBuildToolsPath)\Microsoft.Build.Tasks.v4.0.dll" TaskFactory="CodeTaskFactory">
    <ParameterGroup>
      <Text Output="false" ParameterType="System.String" />
      <Library Output="false" Required="true" ParameterType="System.String" />
      <Value Output="false" Required="true" ParameterType="System.String" />
      <Result Output="true" ParameterType="System.String" />
    </ParameterGroup>
    <Task>
      <Code>Result = ((Text ?? "").Split(';').Contains(Library) ) ? Value : String.Empty;</Code>
    </Task>
  </UsingTask>
  <Target Name="lua51_init">
    <lua51_Contains Text="Linkage-dynamic" Library="lua51" Value="dynamic" Condition="'$(Linkage-lua51)'==''">
      <Output TaskParameter="Result" PropertyName="Linkage-lua51" />
    </lua51_Contains>
    <lua51_Contains Text="Linkage-static" Library="lua51" Value="static" Condition="'$(Linkage-lua51)'==''">
      <Output TaskParameter="Result" PropertyName="Linkage-lua51" />
    </lua51_Contains>
