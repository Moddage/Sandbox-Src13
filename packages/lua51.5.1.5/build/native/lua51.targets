    <lua51_Contains Text="Linkage-sxs" Library="lua51" Value="sxs" Condition="'$(Linkage-lua51)'==''">
      <Output TaskParameter="Result" PropertyName="Linkage-lua51" />
    </lua51_Contains>
    <lua51_Contains Text="CallingConvention-cdecl" Library="lua51" Value="cdecl" Condition="'$(CallingConvention-lua51)'==''">
      <Output TaskParameter="Result" PropertyName="CallingConvention-lua51" />
